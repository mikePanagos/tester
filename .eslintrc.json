{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "jest": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": [
            "off"
        ],
        "semi": [
            "error",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ]

    },
	"parserOptions": {
		"ecmaVersion": 2015,
        "sourceType": "module"
    }
}
 
